n = int(input())
def fn():
    dic={}
    m=int(input())
    lst=list(map(int,input().split()))
    for i in lst:
        dic[i]=True
    k=int(input())
    klst=list(map(int,input().split()))
    for i in klst:
        if i in dic:
            print(1)
        else:
            print(0)
for i in range(n):
    fn()
